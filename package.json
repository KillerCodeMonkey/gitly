{
  "name": "gitly",
  "version": "2.0.3",
  "description": "An API to download and/or extract git repositories",
  "repository": "github:iwatakeshi/gitly",
  "author": "Takeshi Iwana <iwatakeshi@gmail.com>",
  "license": "MIT",
  "main": "dist/index",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=13"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "docs": "ts-readme"
  },
  "module": "dist/gitly.esm.js",
  "size-limit": [
    {
      "path": "dist/gitly.cjs.production.min.js",
      "limit": "15 KB"
    },
    {
      "path": "dist/gitly.esm.js",
      "limit": "15 KB"
    }
  ],
  "devDependencies": {
    "@prismify/tsdx": "^0.1.0",
    "@size-limit/preset-small-lib": "^4.10.2",
    "@types/jest": "^26.0.23",
    "@types/shelljs": "^0.8.8",
    "@types/tar": "^4.0.4",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "shelljs": "^0.8.4",
    "size-limit": "^4.10.2",
    "ts-jest": "^26.5.5",
    "ts-readme": "^1.1.3",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "tar": "^6.1.0"
  }
}
