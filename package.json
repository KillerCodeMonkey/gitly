{
  "name": "gitly",
  "version": "2.1.5",
  "description": "An API to download and/or extract git repositories",
  "repository": "github:iwatakeshi/gitly",
  "author": "Takeshi Iwana <iwatakeshi@gmail.com>",
  "license": "MIT",
  "main": "dist/index",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
    "tar": "^6.1.11"
    "node": ">=13"
  "engines": {
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "docs": "ts-readme"
  },
  "module": "dist/gitly.esm.js",
  "size-limit": [
    {
      "path": "dist/gitly.cjs.production.min.js",
      "limit": "15 KB"
    },
    {
      "path": "dist/gitly.esm.js",
      "limit": "15 KB"
    }
  ],
  "devDependencies": {
    "jest": "^26.6.3",
    "@types/tar": "^4.0.4",
    "@types/shelljs": "^0.8.8",
    "@types/jest": "^26.0.23",
    "@size-limit/preset-small-lib": "^4.10.2",
    "@prismify/tsdx": "^0.1.1",
    "shelljs": "^0.8.4",
    "ts-jest": "^26.5.5",
    "ts-readme": "^1.1.3",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "scripts": {
    "prepare": "yarn build",
    "preversion": "yarn run lint",
    "prepublishOnly": "yarn test",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint -c .eslintrc.js --ext .ts src",
    "lint:fix": "yarn lint --fix"
  },
  "keywords": [
    "archive",
    "bitbucket",
    "download",
    "extract",
    "fetch",
    "git",
    "github",
    "gitlab",
    "offline-first",
    "offline",
    "project",
    "repo",
    "repository",
    "scaffold",
    "tarbarll",
    "zip"
  ]
}
